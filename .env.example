# Discord Bot Environment Variables
# Copy this file to .env and fill in your values

# Discord Configuration
DISCORD_TOKEN=your_bot_token_here
DISCORD_DEV_GUILD_ID=your_development_guild_id_here
DISCORD_REGISTER_GLOBALLY=false

# Environment
DOTNET_ENVIRONMENT=Development

# Optional: Override logging levels
# DISCORDBOT_Logging__MinimumLevel=Debug

# Optional: Override health check settings
# DISCORDBOT_HealthChecks__Enabled=true
# DISCORDBOT_HealthChecks__MemoryThresholdMB=256

# Optional: Override resilience settings
# DISCORDBOT_Resilience__MaxRetryAttempts=3
# DISCORDBOT_Resilience__RetryDelayMs=1000